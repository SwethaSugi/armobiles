{
    "name": "armobiles-root",
    "version": "1.0.0",
    "description": "Root project for ARmobiles application",
    "scripts": {
        "install-all": "npm install && cd client && npm install && cd ../server && npm install",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && npm run dev",
        "client": "cd client && npm run dev",
        "seed-data": "cd server && node scripts/createDataWorkbook.js"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}